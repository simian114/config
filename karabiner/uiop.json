{
  "title": "sanam's Hyper with capslock",
  "maintainers": [
    "sanam"
  ],
  "author": "simian114@gmail.com",
  "rules": [
    {
      "description": "Hyper uiop",
      "manipulators": [
        {
          "description": "u === (",
          "type": "basic",
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "description": "i === )",
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "description": "o === {",
          "type": "basic",
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        },
        {
          "description": "p === }",
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "left_shift"
              ]
            }
          ]
        }
