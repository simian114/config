{
  "title": "sanam's Hyper with capslock",
  "maintainers": [
    "sanam"
  ],
  "author": "simian114@gmail.com",
  "rules": [
    {
      "description": "Hyper mission control",
      "manipulators": [
        {
          "description": "tab == control up_arrow",
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        },
        {
          "description": "e == control right_arrow",
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        },
        {
          "description": "q == control left_arrow",
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        },
        {
          "description": "option q == move previous display",
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control",
                "left_command",
                "left_option"
              ]
            }
          ]
        },
        {
          "description": "command e == move next display",
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_control",
                "left_command",
                "left_option"
              ]
            }
          ]
        },
        {
          "description": "command q == left half",
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_command",
                "left_option"
              ]
            }
          ]
        },
        {
          "description": "command e == right half",
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "right_command",
                "right_control",
                "right_shift",
                "right_option",
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_command",
                "left_option"
              ]
            }
          ]
        }
      ]
    }
  ]
}