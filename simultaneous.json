{
  "title": "sanam's Hyper with capslock",
  "maintainers": [
    "sanam"
  ],
  "author": "simian114@gmail.com",
  "rules": [
    {
      "description": "Hyper simultaneous mission control",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "q"
              },
              {
                "key_code": "w"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -b com.apple.systempreferences /System/Library/PreferencePanes/Sound.prefPane"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "a"
              },
              {
                "key_code": "s"
              },
              {
                "key_code": "d"
              }
            ]
          },
          "to": [
            {
              "key_code": "mission_control"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "z"
              },
              {
                "key_code": "x"
              },
              {
                "key_code": "c"
              }
            ]
          },
          "to": [
            {
              "key_code": "down_arrow",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    }
  ]
}